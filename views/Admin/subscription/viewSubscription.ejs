<%- include('../common/header') %>
  <%- include('../common/sidebar') %>


    <!-- BEGIN: Content-->
    <div class="app-content content ">

      <div class="content-wrapper container-xxl p-0">
        <div class="content-body">
          <section id="multiple-column-form">
            <div class="row justify-content-center">
              <div class="col-12 ">
                <div class="card">
                  <div class="card-header border-bottom">
                    <h4 class="card-title"> View Subscription</h4>
                  </div>
                  <br>
                  <div class="card-body">
                    <form class="form form-vertical" id="" action="" method="POST"
                      enctype="multipart/form-data">
                      <div class="mb-1 row">
                        <div class="col-sm-2">
                          <label class="col-form-label new_lable" for="name">Name</label>
                        </div>
                        <div class="col-sm-10">
                          <input type="text" id="name" class="form-control" name="name" value="<%=subdetails.name%> "
                            disabled>
                        </div>
                      </div>
                      <div class="mb-1 row">
                        <div class="col-sm-2">
                          <label class="col-form-label new_lable" for="email">Price</label>
                        </div>
                        <div class="col-sm-10">
                          <input type="text" id="price" class="form-control" name="price" value="<%=subdetails.price%> "
                            disabled>
                        </div>
                      </div>
                      <div class="mb-1 row">
                        <div class="col-sm-2">
                          <label class="col-form-label new_lable" for="phone">Tenure</label>
                        </div>
                        <div class="col-sm-10">
                          <input type="text" id="tenure" class="form-control" name="tenure"
                            value="<%=subdetails.tenure%> " disabled>
                        </div>
                      </div>
                      <div class="row">
                      <div class="col-12 text-end">
                          <a href="/subscriptionList" class="btn btn-primary float-right" title="Edit Profile"> Back
                          </a>
                        </div>
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
          </section>
        </div>
      </div>
    </div>
    <div class="sidenav-overlay"></div>
    <div class="drag-target"></div>
    <%- include('../common/footer') %>